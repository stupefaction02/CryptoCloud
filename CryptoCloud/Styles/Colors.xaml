<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="First">#45454A</Color>
    <Color x:Key="Second">#1B1C21</Color>
    <Color x:Key="Third">#707383</Color>
    <Color x:Key="Fourth">#FFFFFF</Color>
    <Color x:Key="Fifth">#222329</Color>
    <Color x:Key="Sixth">#2886F1</Color>

    <SolidColorBrush x:Key="SecondPanelBrush" Color="{StaticResource Second}"/>
    <SolidColorBrush x:Key="ThirdPanelBrush" Color="{StaticResource Fifth}"/>
    
    <SolidColorBrush x:Key="TextBlockLinkBrush" Color="{StaticResource Sixth}"/>
    
    <SolidColorBrush x:Key="FirstPanelBrush" Color="{StaticResource First}"/>
    <SolidColorBrush x:Key="FirstTextBlockBrush" Color="{StaticResource First}"/>
    <SolidColorBrush x:Key="SecondTextBlockBrush" Color="{StaticResource Fourth}"/>

</ResourceDictionary>